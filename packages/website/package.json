{
  "name": "website",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "open http://localhost:3000 && next dev",
    "dev": "cp env/.env.development .env.local && yarn start",
    "dev:staging": "cp env/.env.staging .env.local && yarn start",
    "dev:production": "cp env/.env.production .env.local && yarn start",
    "lint:prettier": "prettier -c \"**/*.{ts,tsx}\"",
    "lint:eslint": "eslint --ext [.ts,.tsx] .",
    "lint:ts": "tsc",
    "lint": "npm-run-all --parallel lint:*",
    "build": "cp \"env/.env.$MODE\" .env.local && next build"
  },
  "dependencies": {
    "@heroicons/react": "2.0.18",
    "@tanstack/react-query": "4.36.1",
    "colors": "*",
    "components": "*",
    "firebase": "10.2.0",
    "firebase-admin": "11.11.0",
    "gray-matter": "4.0.3",
    "jotai": "^2.5.1",
    "mixpanel-browser": "2.47.0",
    "next": "13.4.16",
    "next-mdx-remote": "4.4.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hot-toast": "2.4.1",
    "utils": "*"
  },
  "devDependencies": {
    "@svgr/webpack": "8.1.0",
    "@tanstack/react-query-devtools": "4.36.1",
    "@total-typescript/ts-reset": "0.4.2",
    "@types/react": "18.2.8",
    "@types/react-dom": "18.2.4",
    "autoprefixer": "10.4.12",
    "cross-env": "7.0.3",
    "eslint": "8.48.0",
    "eslint-config-custom": "*",
    "eslint-plugin-react": "7.31.11",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-tailwindcss": "3.12.1",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.24",
    "prettier": "3.0.3",
    "prettier-plugin-tailwindcss": "0.4.1",
    "tailwind-merge": "1.14.0",
    "tailwindcss": "3.3.3",
    "tsconfig": "*",
    "types": "*",
    "typescript": "5.2.2"
  }
}
