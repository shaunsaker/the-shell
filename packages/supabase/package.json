{
  "name": "supabase",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev:stripe": "stripe listen --forward-to localhost:54321/functions/v1/stripe-webhook -H 'Stripe-Version: 2022-11-15'",
    "dev:functions": "supabase functions serve --no-verify-jwt --env-file ./supabase/functions/.env.local",
    "dev": "npm-run-all --parallel dev:*",
    "build:types": "supabase gen types typescript --local --schema public > ../app/src/types/supabase.ts && prettier --write ../app/src/types/supabase.ts",
    "build": "npm-run-all --parallel build:*",
    "db:migration": "supabase db diff -f"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.0"
  }
}
