{
  "name": "ultimate-b2b-saas-boilerplate",
  "description": "The ultimate boilerplate for building SAAS applications with React, Supabase, Stripe, Tailwind CSS and TypeScript.",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "prepare": "husky install",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "build": "turbo run build"
  },
  "devDependencies": {
    "eslint-config-custom": "*",
    "husky": "^8.0.2",
    "lint-staged": "^13.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.1",
    "turbo": "latest"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ]
  },
  "packageManager": "yarn@1.22.19",
  "workspaces": [
    "packages/*"
  ],
  "dependencies": {
    "@types/node": "^14.18.54",
    "typescript": "^4.9.5"
  }
}
